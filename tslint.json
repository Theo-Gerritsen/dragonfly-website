{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-var-requires": false,
        "no-submodule-imports": false,
        "no-implicit-dependencies": false,
        "no-string-literal": false,
        "no-bitwise": false,
        "variable-name": false
    },
    "rulesDirectory": []
}