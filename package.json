{
    "name": "dragonfly-website",
    "version": "1.0.0",
    "description": "JavaScript & CSS for the dragonfly website.",
    "main": "index.js",
    "dependencies": {
        "autoprefixer": "^9.7.2",
        "glyphs2font": "^1.2.0",
        "hamburgers": "^1.1.3",
        "postcss-cli": "^6.1.3",
        "tailwindcss": "^1.1.4"
    },
    "devDependencies": {
        "css-loader": "^3.2.0",
        "file-loader": "^5.0.2",
        "mini-css-extract-plugin": "^0.8.0",
        "postcss": "^7.0.23",
        "postcss-font-magician": "^2.3.1",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "precss": "^4.0.0",
        "webpack": "^4.41.2"
    },
    "scripts": {
        "fonts": "glyphs2font fonts.yaml",
        "build": "npm run css & npm run fonts",
        "css": "NODE_ENV=production postcss content/stylesheets/main.src.css --config ./ -o content/stylesheets/dragonfly.css",
        "watch:css": "npm run fonts & NODE_ENV=production postcss content/stylesheets/main.src.css -m --config ./ -o content/stylesheets/dragonfly.css -w"
